digraph grammar {
	graph [bgcolor=aliceblue rankdir=TB]
	"int x = 0 " [color=blue fontcolor=blue]
	beginCode -> "int x = 0 "
	"int m = x * 3 " [color=blue fontcolor=blue]
	"int x = 0 " -> "int m = x * 3 "
	" h = m+1" [color=green fontcolor=green]
	"int m = x * 3 " -> " h = m+1"
	" x = h+2" [color=green fontcolor=green]
	" h = m+1" -> " x = h+2"
	" x = h+2" -> endCode
}
